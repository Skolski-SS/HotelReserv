<apex:page controller="AddReservPageController">
    <apex:form id="addReserv">
        <apex:pageBlock title="Add Reserv">
            <apex:pageMessages id="showmsg"></apex:pageMessages>
            <apex:pageBlockSection columns="1">
                <apex:inputField html-disabled="true" value="{!reservroom.Hotel__c}" />                 
                <apex:inputField html-disabled="true" value="{!reserv.Room__c}"/>
                <apex:inputField value="{!reserv.ArrivalDate__c}"/>
                <apex:inputField value="{!reserv.DepartureDate__c}" />
                <apex:inputField value="{!reserv.ContactEmail__c}" />
                <apex:inputField value="{!reserv.NumberOfGuest__c}" />
                <apex:inputField value="{!reservroom.Alcohol__c}" />
                <apex:inputField value="{!reservroom.Internet__c}" />
                <apex:inputField value="{!reservroom.Sauna__c}" />
                </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton value="Add Reserv" action="{!save}"/>
                <apex:commandButton value="Cancel" action="{!cancel}"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>